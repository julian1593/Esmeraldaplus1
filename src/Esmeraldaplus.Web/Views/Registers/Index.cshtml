
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

@section Vista_Genelar {
    <link rel="stylesheet" href="../css/register.css">
}

<form method="post" onsubmit="return validar()" class="form-inicio-sesion">
    <h2 class="titulo">REGISTRARSE</h2>
    <div class="contenido-input">
        <input type="text" id="nombre1" placeholder="Primer Nombre" class="input-48">

        <input type="text" id="nombre2" placeholder="Segundo Nombre" class="input-48">

        <input type="text" id="apellido1" placeholder="Primer Apellido" class="input-48">

        <input type="text" id="apellido2" placeholder="Sapellido2egundo Apellido" class="input-48">

        <input type="email" id="correo" placeholder="Correo Electronico" class="input-108">

        <input type="password" id="contraseña" placeholder="Contraseña" class="input-48">

        <input type="password" id="recontraseña" placeholder="Repetir Contraseña" class="input-48">

        <select class="doc-form">

            <option selected="" disabled="">Seleccione</option>
            <option>CC</option>
            <option>NIT</option>
            <option>CE</option>
            <option>PP</option>
        </select>

        <input type="number" id="ndocumento" placeholder="Numero de Documento" class="input-64">

        <p><input type="checkbox" class="input-100" required=""> Estoy de acuerdo con <a href="terms and conditions.html">Terminos y Condiciones</a></p>

        <input type="submit" value="Registrarse" onclick="registrar()" class="btn-register">

        <p>¿Ya tienes cuenta? <a asp-area="" asp-controller="Logins" asp-action="Index">Iniciar Sesion</a></p>
    </div>
</form>
@section Scripts {
    <script type="text/javascript" src="http://code.jquery.com/jquery-latest.js"></script>
    <script type="text/javascript" src="../js/register.js"></script>

}

